<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.git.mapper.OBlogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.git.model.entity.OBlog">
        <id column="blog_id" property="blogId" />
        <result column="category_id" property="categoryId" />
        <result column="title" property="title" />
        <result column="summary" property="summary" />
        <result column="status" property="status" />
        <result column="weight" property="weight" />
        <result column="support" property="support" />
        <result column="click" property="click" />
        <result column="header_img" property="headerImg" />
        <result column="type" property="type" />
        <result column="content" property="content" />
        <result column="user_name" property="userName" />
        <result column="create_time" property="createTime" />
        <result column="update_by" property="updateBy" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        blog_id, category_id, title, summary, status, weight, support, click, header_img, type, content, user_name, create_time, update_by, update_time
    </sql>

</mapper>
